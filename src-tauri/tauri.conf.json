{
  "$schema": "https://schema.tauri.app/config/1",
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "clear && bun run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "serenitymc.ru Launcher",
  "mainBinaryName": "serenitymc.ru Launcher",
  "version": "0.2.6",
  "identifier": "ru.serenitymc.launcher",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "серенити",
        "width": 800,
        "height": 500,
        "decorations": false,
        "resizable": false,
        "center": true
      }
    ]
  }
}